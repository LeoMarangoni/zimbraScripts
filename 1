#deb http://ftp.br.debian.org/debian/ jessie main
#deb-src http://ftp.br.debian.org/debian/ jessie main

deb http://ftp.br.debian.org/debian/ jessie main non-free
deb-src http://ftp.br.debian.org/debian/ jessie main non-free

deb http://security.debian.org/debian-security jessie/updates main contrib
deb-src http://security.debian.org/debian-security jessie/updates main contrib

# jessie-updates, previously known as 'volatile'
deb http://ftp.br.debian.org/debian/ jessie-updates main contrib
deb-src http://ftp.br.debian.org/debian/ jessie-updates main contrib


#deb http://ftp.br.debian.org/debian/ stretch main
#deb-src http://ftp.br.debian.org/debian/ stretch main

deb http://ftp.br.debian.org/debian/ stretch main non-free
deb-src http://ftp.br.debian.org/debian/ stretch main non-free

deb http://security.debian.org/debian-security stretch/updates main contrib
deb-src http://security.debian.org/debian-security stretch/updates main contrib

# stretch-updates, previously known as 'volatile'
deb http://ftp.br.debian.org/debian/ stretch-updates main contrib
deb-src http://ftp.br.debian.org/debian/ stretch-updates main contrib

